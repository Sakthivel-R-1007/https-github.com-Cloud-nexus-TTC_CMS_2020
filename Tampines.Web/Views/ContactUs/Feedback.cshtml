@section scripts{



    <script type="text/javascript" src="@appPath/Contents/scripts/jqueryValidate/jquery.validate.js"></script>
    <script type="text/javascript" src="@appPath/Contents/scripts/jqueryValidate/jquery.validate.additional-methods.js"></script>







}


<script>
    $('input[name=uploadAttach]').change(function () {
        var fileName = document.getElementById("uploadAttach").files.item(0).name;
        document.getElementById("uploadFile").value = fileName;
    });

    



    $(document).ready(function () {

        //$(".Numeric").numeric({
        //    allowMinus: false,
        //    allowThouSep: false
        //});

        loadCaptcha();
        $("#reload").click(loadCaptcha);
        IntializeValidation();
       
    });
    function IntializeValidation() {

        jQuery.validator.addMethod('filesize', function (value, element, param) {
            return this.optional(element) || (element.files[0].size <= param)
        }, 'File size must be less than 5MB');

        //jQuery.validator.addMethod('lessthen', function (value, element, param) {
        //	return this.optional(element) || parseInt(value) <= parseInt($(param).val());
        //}, 'Invalid value');

        $("#FeedBackForm").validate({
            errorPlacement: function (error, element) {
                $("span[data-valmsg-for='" + $(element).attr("name") + "']").html($(error));
            },
            rules: {
                feedbackType: {
                    required: true
                },
                Name: {
                    required: true,

                },
                NRIC: {
                    required: true,
                    minlength: 4,
                    maxlength: 4
                },
                Email: {
                    required: true,
                    email: true
                },
                ContactNo: {
                    required: true,
                    minlength: 8,
                    maxlength: 8
                },
                Block: {
                    required: true
                },
                Floor: {
                    required: true
                },
                UnitNo: {
                    required: true
                },
                RoadName: {
                    required: true
                },
                PostalCode: {
                    required: true,
                    minlength: 6,
                    maxlength: 6
                },
                FeedbackText: {
                    required: true
                },
                uploadAttach: {

                    extension: 'jpg|jpeg|png',
                    filesize: 2048000 /*5MB*/
                },
                captcha: {
                    required: true
                }

            },
            messages: {

                ContactNo: {
                    minlength: "Please enter 8 digit number.",
                    maxlength: "Please enter 8 digit number."
                },
                PostalCode: {
                    minlength: "Please enter 6 digit number.",
                    maxlength: "Please enter 6 digit number."
                }
            }
        });
    }

    $("input[name='uploadAttach']").change(function () {

        var filename = $(this).val();
        if (filename.substring(3, 11) == 'fakepath') {
            filename = filename.substring(12);
        }
        document.getElementById("uploadFile").value = filename;
    });


    function loadCaptcha() {
        $("#imgCap").val('');
        $.ajax({
            type: "GET",
            url: apppath + '/ContactUs/RefreshCaptcha',
            contentType: "image/png",
            success: function (data) {
                $('#imgCap').attr('src', "data:image/png;base64," + data.captchaImage);
                $("#SecurityCode").val(data.code);
            },
            error: function (error, txtStatus) {
                console.log(txtStatus);
            }
        });
    }

</script>

